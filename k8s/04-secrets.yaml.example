# Copy to 04-secrets.yaml and fill in real values. NEVER commit 04-secrets.yaml.
apiVersion: v1
kind: Secret
metadata:
  name: openclaw-secrets
  namespace: openclaw
type: Opaque
stringData:
  GATEWAY_TOKEN: "<your-gateway-token>"
  OPENAI_API_KEY: "<your-openai-key>"
  GOOGLE_API_KEY: "<your-google-key>"
  ORACLE_API_KEY: "<your-oracle-key>"
